<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1880.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="milestone2.FXMLController">
   <children>
      <Label layoutX="574.0" layoutY="14.0" text="Edmonton Property Assessments">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="574.0" layoutY="54.0" prefHeight="613.0" prefWidth="1265.0">
        <columns>
          <TableColumn fx:id="account" prefWidth="150.0" text="Account" />
          <TableColumn fx:id="address" prefWidth="200.0" text="Address" />
            <TableColumn fx:id="assessedValue" prefWidth="200.0" text="Assessed Value" />
            <TableColumn fx:id="assessmentClass" prefWidth="200.0" text="assessment Class" />
            <TableColumn fx:id="neighbourhood" prefWidth="275.0" text="Neighbourhood" />
            <TableColumn fx:id="longitude" prefWidth="125.0" text="Latitude" />
            <TableColumn fx:id="latitude" prefWidth="125.0" text="longitude" />
        </columns>
      </TableView>
      <VBox layoutX="6.0" prefHeight="720.0" prefWidth="542.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Find Property Assessment">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Account Number:" />

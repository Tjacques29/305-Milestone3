               <VBox.margin>
                  <Insets left="75.0" top="-35.0" />
               </VBox.margin>
            </Button>
            <TextArea fx:id="propertyTextArea" prefHeight="200.0" prefWidth="200.0" />
            <Button mnemonicParsing="false" onAction="#addDeleteSenceButton" text="Add Delete" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </children>
